{
  "name": "template-reactjs",
  "version": "1.0.0",
  "description": "Template client side with Reactjs at create-react-app",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/i-espinola/template-reactjs.git"
  },
  "bugs": {
    "url": "https://github.com/i-espinola/template-reactjs/issues"
  },
  "readme": "https://github.com/i-espinola/template-reactjs#readme",
  "license": "MIT",
  "main": "server/index.js",
  "homepage": ".",
  "scripts": {
    "clean": "rm -rf build package-lock.json npm-debug.log",
    "start": "npm run build && node server/index.js",
    "build": "npm run clean && react-scripts build",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "test": "react-scripts test"
  },
  "dependencies": {
    "react": "*",
    "react-dom": "*",
    "react-scripts": "*",
    "prop-types": "*",
    "serve-handler": "*",
    "react-router-dom": "*"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "*",
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-config-standard": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-node": "*",
    "eslint-plugin-promise": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-standard": "*",
    "node-sass": "*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
